{
  "name": "servai-frontend",
  "version": "1.0.0",
  "description": "ServAI - AI Property Management Platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "NODE_ENV=production vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "quasar": "^2.14.2",
    "@quasar/extras": "^1.16.11",
    "axios": "^1.6.5",
    "vue-i18n": "^9.9.0",
    "chart.js": "^4.4.1",
    "vue-chartjs": "^5.3.0",
    "date-fns": "^3.2.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "vite": "^5.0.11",
    "@quasar/vite-plugin": "^1.6.0",
    "vite-plugin-pwa": "^0.19.0",
    "sass": "^1.70.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.4",
    "vitest": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "@vue/test-utils": "^2.4.3",
    "@playwright/test": "^1.41.0",
    "happy-dom": "^13.3.5"
  }
